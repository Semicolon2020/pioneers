            <Font name="Bell MT Bold" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="rec" layoutX="496.0" layoutY="279.0" opacity="0.88" prefHeight="111.0" prefWidth="399.0" promptText="Taper ici votre réclamation..." />
